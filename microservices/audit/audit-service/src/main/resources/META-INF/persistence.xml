<?xml version="1.0" encoding="UTF-8"?>
<persistence
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        version="2.2"
        xmlns="http://xmlns.jcp.org/xml/ns/persistence"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
    <persistence-unit name="audit.PU" transaction-type="JTA">
        <jta-data-source>jdbc/crm_audit</jta-data-source>
        <class>org.hsh.crm.ms.common.dal.entities.Setting</class>
        <class>org.hsh.crm.ms.audit.dal.entities.BaseDataAudit</class>
        <class>org.hsh.crm.ms.audit.dal.entities.DataAudit1</class>
        <class>org.hsh.crm.ms.audit.dal.entities.DataAudit2</class>
        <class>org.hsh.crm.ms.audit.dal.entities.DataAuditHistory</class>
        <class>org.hsh.crm.ms.audit.dal.entities.BaseSecurityAudit</class>
        <class>org.hsh.crm.ms.audit.dal.entities.SecurityAudit1</class>
        <class>org.hsh.crm.ms.audit.dal.entities.SecurityAudit2</class>
        <class>org.hsh.crm.ms.audit.dal.entities.SecurityAuditHistory</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
        <properties/>
    </persistence-unit>
</persistence>
